    "document-ready": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "globby": "^7.1.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.6",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "moment": "^2.20.1",
    "react": "^16.2.0",
