    "husky": "^0.14.3",
    "jest": "^22.4.0",
    "less": "^3.0.1",
    "less-loader": "^4.0.5",
    "react": "^16.2.0",
    "react-codemirror2": "^4.0.0",
    "react-disqus-comments": "^1.1.1",
    "react-dom": "^16.2.0",
    "react-feather": "^1.0.8",
    "react-markdown": "^3.2.0",
    "react-qr-svg": "^2.1.0",
    "react-router-dom": "^4.2.2",
