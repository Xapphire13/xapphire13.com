{
  "name": "xapphire13.com",
  "version": "1.0.0",
  "description": "Xapphire13.com",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "bundle": "node ./scripts/bundle.js",
    "clean": "rmdir /S /Q .\\dist",
    "build": "npm run clean && tsc -p ./src && xcopy /Y /E /I \"./src/sql\" \"./dist/sql\" && webpack --config webpack.dev.js",
    "build:prod": "tsc -p ./src && xcopy /Y /E /I \"./src/sql\" \"./dist/sql\" && webpack --config webpack.prod.js",
    "start": "node ./dist/index.js",
    "start:debug": "node --inspect-brk ./dist/index.js",
    "start:watch": "node ./scripts/watch.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xapphire13/xapphire13.com.git"
  },
  "keywords": [
    "xapphire13",
    "website"
  ],
  "author": "Xapphire13",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xapphire13/xapphire13.com/issues"
  },
  "homepage": "https://github.com/Xapphire13/xapphire13.com#readme",
  "jest": {
    "mapCoverage": true,
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tests/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/tests/.*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
