    "otplib": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.6",
    "semaphore-async-await": "^1.5.1",
    "sqlite": "^2.9.0",
    "typedi": "^0.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
