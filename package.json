{
  "name": "xapphire13.com",
  "version": "1.0.0",
  "description": "Xapphire13.com",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "tslint -p src && tslint -p src/app && tslint -p tests",
    "test": "npm run lint && jest",
    "bundle": "node ./scripts/bundle.js",
    "clean": "rimraf ./dist",
    "copy-sql": "xcopy /Y /E /I \"./src/sql\" \"./dist/sql\"",
    "build": "npm run clean && tsc -p ./src && npm run copy-sql && webpack --config webpack.dev.js",
    "build:prod": "npm run clean && tsc -p ./src && npm run copy-sql && webpack --config webpack.prod.js",
    "start": "node ./dist/index.js",
    "start:debug": "node --inspect-brk ./dist/index.js",
    "start:watch": "node ./scripts/watch.js",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xapphire13/xapphire13.com.git"
  },
  "keywords": [
    "xapphire13",
    "website"
  ],
  "author": "Xapphire13",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xapphire13/xapphire13.com/issues"
  },
  "homepage": "https://github.com/Xapphire13/xapphire13.com#readme",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tests/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/tests/.*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@octokit/rest": "^15.1.7",
    "bcrypt": "^2.0.0",
    "body-parser": "^1.18.2",
    "boom": "^7.1.1",
    "crc-32": "^1.2.0",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "jsonwebtoken": "^8.1.1",
    "moment": "^2.20.1",
    "node-fetch": "^2.0.0",
    "otplib": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.6",
    "semaphore-async-await": "^1.5.1",
    "sqlite": "^2.9.1",
    "tsyringe": "^2.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/boom": "^7.1.1",
    "@types/codemirror": "0.0.56",
    "@types/common-tags": "^1.4.0",
    "@types/cross-storage": "^0.8.29",
    "@types/delay": "^2.0.1",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.1",
    "@types/globby": "^6.1.0",
    "@types/halogen": "^0.2.5",
    "@types/handlebars": "^4.0.36",
    "@types/jest": "^22.1.3",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/node-fetch": "^1.6.7",
    "@types/otplib": "^7.0.0",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-router-dom": "^4.2.4",
    "@types/react-table": "^6.7.3",
    "@types/react-tagsinput": "^3.19.2",
    "codemirror": "^5.35.0",
    "common-tags": "^1.7.2",
